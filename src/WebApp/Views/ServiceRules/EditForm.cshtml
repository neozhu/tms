﻿@model WebApp.ServiceRule
<!-- begin master section -->
<div  class="main-form">
<!--begin servicerule form  -->
<form id="servicerule_form" method="post" novalidate="novalidate" >
@Html.AntiForgeryToken()
   @Html.HiddenFor(x=>x.Id)
 <fieldset>
 <div class="form-group form-group-sm">
      <div class="row">

        <!--服务项目-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.NAME)</span> </label>
			@Html.EditorFor(x => x.NAME, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.NAME) } })
      
         </div>

		<!--货物总量起-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.GWT1)</span> </label>
            @Html.EditorFor(x => x.GWT1, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.GWT1) } })

        </div>

		<!--货物总量至-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.GWT2)</span> </label>
            @Html.EditorFor(x => x.GWT2, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.GWT2) } })

        </div>

		
      <!--电梯楼层-->
      <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
              <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.ELEVATOR)</span> </label>
              <div class="checkbox  no-margin">
              <label>
                 <input id="@Html.IdFor(model => model.ELEVATOR)"
                       name="@Html.NameFor(model => model.ELEVATOR)"
                       value="true" @(Model.ELEVATOR ? "checked=\"checked\"" : "")
                       class="checkbox style-0"
                       tabindex="6"
                       type="checkbox" />
                <span>@Html.DisplayNameFor(model => model.ELEVATOR)</span>
              </label>
            </div>
      </div>

		<!--单价-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.PRICE)</span> </label>
            @Html.EditorFor(x => x.PRICE, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.PRICE) } })

        </div>

		<!--最小收费-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.MINAMOUNT)</span> </label>
            @Html.EditorFor(x => x.MINAMOUNT, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.MINAMOUNT) } })

        </div>
	   </div>
    </div>
 <div class="form-group form-group-sm">
      <div class="row">

		<!--状态-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.STATUS)</span> </label>
            @Html.EditorFor(x => x.STATUS, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.STATUS) } })

        </div>

        <!--说明-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.DESCRIPTION) </label>
			@Html.EditorFor(x => x.DESCRIPTION, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.DESCRIPTION) } })
      
         </div>

        <!--LOTTABLE1-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOTTABLE1) </label>
			@Html.EditorFor(x => x.LOTTABLE1, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.LOTTABLE1) } })
      
         </div>

        <!--LOTTABLE2-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label">@Html.DisplayNameFor(model => model.LOTTABLE2) </label>
			@Html.EditorFor(x => x.LOTTABLE2, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.LOTTABLE2) } })
      
         </div>

		<!--LOTTABLE3-->
        <div class="col-xs-12 col-sm-6 col-md-3 col-lg-2">
            <label class="control-label"><span class="required">@Html.DisplayNameFor(model => model.LOTTABLE3)</span> </label>
            @Html.EditorFor(x => x.LOTTABLE3, new { htmlAttributes = new { @class = "form-control",@type="number", placeholder = Html.DisplayNameFor(x => x.LOTTABLE3) } })

        </div>
	    
   </div>
  </div>
</fieldset>
</form>
<!-- end servicerule form  -->
</div>

<!-- end master section -->
<!--========================================================================================-->

