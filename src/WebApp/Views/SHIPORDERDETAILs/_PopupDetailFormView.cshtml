<script type="text/javascript">
    //判断新增编辑状态
    var MODELSTATE = 'Added';
    var shiporderdetailid = null;
    var hasChanged = false;
    function opendetailwindow(data,state) {
         MODELSTATE = state;
         initdetailview();
         shiporderdetailid = (data.ID || 0);
         $("#detailswindow").window("open");
         $('#shiporderdetail_form').form('reset');
         $('#shiporderdetail_form').form('load', data);
             }
  //删除当前记录
  function deleteitem() {
    $.messager.confirm('确认', '你确定要删除该记录?', result => {
      if (result) {
        const url = `/SHIPORDERDETAILs/DeleteAsync/${shiporderdetailid}`;
        $.get(url).done(res => {
          if (res.success) {
            toastr.success("删除成功");
            $dg.datagrid('reload');
            $("#detailswindow").window("close");
          } else {
            $.messager.alert("错误", res.err, "error");
          }
